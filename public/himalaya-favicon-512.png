<svg xmlns="http://www.w3.org/2000/svg" width="512" height="512" viewBox="0 0 400 400">
  <rect width="400" height="400" fill="#000000"/>
  
  <!-- Sun rays -->
  <g stroke="#ffffff" stroke-width="3" fill="none">
    <line x1="200" y1="40" x2="200" y2="60"/>
    <line x1="230" y1="45" x2="225" y2="62"/>
    <line x1="255" y1="60" x2="245" y2="75"/>
    <line x1="275" y1="80" x2="262" y2="92"/>
    <line x1="290" y1="105" x2="275" y2="115"/>
    <line x1="170" y1="45" x2="175" y2="62"/>
    <line x1="145" y1="60" x2="155" y2="75"/>
    <line x1="125" y1="80" x2="138" y2="92"/>
