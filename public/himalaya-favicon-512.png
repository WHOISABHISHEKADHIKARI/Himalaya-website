    <line x1="50" y1="240" x2="350" y2="240"/>
    <line x1="50" y1="250" x2="350" y2="250"/>
    <line x1="50" y1="260" x2="350" y2="260"/>
    <line x1="50" y1="270" x2="350" y2="270"/>
    <line x1="50" y1="280" x2="350" y2="280"/>
  </g>
  
  <!-- HIMALAYA text -->
  <text x="200" y="320" font-family="Arial, sans-serif" font-size="32" font-weight="bold" text-anchor="middle" fill="#ffffff">HIMALAYA</text>
  
  <!-- KRISHI TATHA PASUPALAN FARM text -->
  <text x="200" y="345" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#ffffff">KRISHI TATHA PASUPALAN</text>
  <text x="200" y="360" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#ffffff">FARM</text>
  
